<% if flash[:errors]%>
<% flash[:errors].each do |error|%>
<%= error %>
<% end %>
<% end %>
<h1> Create New Post</h1>
<%= form_for @post do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :content %>
<%= f.text_field :content %>
<%= f.collection_select :blogger, Blogger.all, :id, :name %>
<%= f.collection_select :destination, Destination.all, :id, :name %>
<%= f.submit %>

<% end %>